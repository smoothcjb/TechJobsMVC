<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (Dictionary<string, string> job in ViewBag.jobs)

{
    <table class="job-listing">
        @for (int i = 0; i < job.Count; i++)

        {
            <tr>
                <td>@job.Keys.ElementAt(i)</td>
                <td> @job[job.Keys.ElementAt(i)]</td>
            </tr>

        }

    </table>
}



